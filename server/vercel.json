{
  "builds": [
    {
      "src": "app.js",
      "use": "@vercel/node"
    },
    {
      "src": "frontend/dist/**/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "^/(.*\\..*)$",
      "dest": "/frontend/dist/$1"
    },
    {
      "src": "^(?!/api|/frontend).*",
      "dest": "/frontend/dist/index.html"
    },
    {
      "src": "^/(.*)",
      "dest": "/app.js"
    }
  ]
}