{
    "builds": [
      {
        "src": "server/app.js",
        "use": "@vercel/node"
      },
      {
        "src": "frontend/dist/**/*",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      {
        "src": "^/(.*)",
        "dest": "/server/app.js"
      },
      {
        "src": "^/(.*\\..*)$",
        "dest": "/frontend/dist/$1"
      },
      {
        "src": "^/(.*)$",
        "dest": "/frontend/dist/index.html"
      }
    ]
  }
  